{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "quotes":"off",
      "arrow-parens":"off",
      "no-console":"off",
      "no-await-in-loop":"off"
    }
}
